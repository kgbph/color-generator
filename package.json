{
  "name": "color-generator",
  "version": "1.0.0",
  "description": "General purpose random color generator",
  "main": "index.js",
  "scripts": {
    "build": "run-s lint uglify",
    "clean": "rimraf dist/*",
    "lint": "eslint src",
    "postinstall": "run-s build",
    "uglify": "mkdirp dist -p && uglifyjs src/*.js -m -o dist/color-generator.js && uglifyjs src/*.js -m -c -o dist/color-generator.min.js",
    "watch": "onchange \"src\" -- run-s build",
    "test": "qunit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kgbph/color-generator.git"
  },
  "keywords": [
    "kgbph",
    "random",
    "rgb",
    "color",
    "generator"
  ],
  "author": "kgbph",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kgbph/color-generator/issues"
  },
  "homepage": "https://github.com/kgbph/color-generator#readme",
  "devDependencies": {
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^2.1.1",
    "onchange": "^2.4.0",
    "qunit": "^2.8.0",
    "rimraf": "^2.5.4",
    "uglify-es": "^3.3.9"
  },
  "fsevents": "*",
  "dependencies": {}
}
